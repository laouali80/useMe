{
  "expo": {
    "name": "UseMe",
    "slug": "useMe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicon1.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2358ea"
      // #2358ea
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon1.png",
        "backgroundColor": "#2358ea"
        // #2358ea
      }
    },
    "web": {
      "favicon": "./assets/favicon1.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow useMe to access your location for better user experience."
        }
      ]
    ]
  }
}
